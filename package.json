{
  "name": "xdesign-ui",
  "version": "0.1.3",
  "private": false,
  "keywords": [
    "MIT",
    "XDesign",
    "xdesign-ui",
    "xdesign-react-ui",
    "ts",
    "react",
    "react-ui",
    "react-ts-ui"
  ],
  "description": "一套专为 React 开发，适合开发者使用的轻量级 UI 组件库",
  "homepage": "https://ruanjiaran.github.io/XDesign",
  "repository": {
    "type": "github",
    "url": "https://github.com/RuanJiaran/XDesign/tree/master"
  },
  "bugs": {
    "url": "https://github.com/RuanJiaran/XDesign/issues"
  },
  "author": {
    "name": "ruanjiaran",
    "email": "1606707347@qq.com"
  },
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:deploy": "npm publish",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "start": "dumi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
